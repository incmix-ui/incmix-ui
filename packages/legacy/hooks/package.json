{
  "name": "@incmix-ui/hooks",
  "version": "2.1.2",
  "description": "React hooks for Chakra components",
  "keywords": [
    "hooks",
    "react",
    "chakra ui",
    "utilities"
  ],
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incmix-ui/incmix-ui.git",
    "directory": "packages/legacy/hooks"
  },
  "bugs": {
    "url": "https://github.com/incmix-ui/incmix-ui/issues"
  },
  "dependencies": {
    "@chakra-ui/react-utils": "workspace:*",
    "@chakra-ui/utils": "workspace:*",
    "compute-scroll-into-view": "1.0.14",
    "copy-to-clipboard": "3.3.1"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "clean-package": "2.1.1"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src/index.ts",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  }
}
