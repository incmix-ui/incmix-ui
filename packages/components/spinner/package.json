{
  "name": "@incmix-ui/spinner",
  "version": "2.0.11",
  "description": "A React component for displaying spinners and loaders",
  "keywords": [
    "spinner",
    "loader",
    "react",
    "chakra ui"
  ],
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
