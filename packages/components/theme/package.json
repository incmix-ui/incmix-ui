{
  "name": "@incmix-ui/theme",
  "version": "2.2.2",
  "description": "The default theme for chakra components",
  "keywords": [
    "theme",
    "theming",
    "ui mode",
    "ui"
  ],
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "foundations": [
        "./dist/foundations/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incmix-ui/incmix-ui.git",
    "directory": "packages/components/theme"
  },
  "bugs": {
    "url": "https://github.com/incmix-ui/incmix-ui/issues"
  },
  "dependencies": {
    "@chakra-ui/theme": "2.2.2"
  },
  "scripts": {
    "build": "tsup src/index.ts src/components/index.ts src/foundations/index.ts --dts",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src/index.ts",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  }
}
