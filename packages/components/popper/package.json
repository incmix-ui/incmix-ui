{
  "name": "@incmix-ui/popper",
  "version": "3.0.11",
  "description": "A React component and hooks wrapper for popper.js",
  "keywords": [
    "react",
    "popper",
    "popover",
    "tooltips",
    "popper.js",
    "positioning",
    "popperjs-modifier",
    "ui",
    "incmix ui",
    "component"
  ],
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@chakra-ui/popper": "3.0.11"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
