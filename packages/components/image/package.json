{
  "name": "@incmix-ui/image",
  "version": "2.0.12",
  "description": "React component for progressive image loading",
  "keywords": [
    "react",
    "progressive",
    "lazyloading",
    "component",
    "chakra ui",
    "image",
    "loading"
  ],
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
