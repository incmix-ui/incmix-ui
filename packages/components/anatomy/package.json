{
  "name": "@incmix-ui/anatomy",
  "version": "2.1.0",
  "description": "The anatomy of all chakra components",
  "keywords": [
    "theme",
    "theming",
    "ui mode",
    "ui"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
